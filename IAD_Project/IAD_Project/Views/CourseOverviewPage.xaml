<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="IAD_Project.Views.CourseOverviewPage">
    <ContentPage.Content>
        <StackLayout Style="{StaticResource stkOuter}">

            <StackLayout Style="{StaticResource stkWarning}">
                
            </StackLayout>

            <StackLayout Style="{StaticResource stkTitle}">
                <Label x:Name="lblTitle" Text="" />
            </StackLayout>

            <StackLayout Style="{StaticResource stkContent} " x:Name="layout">

            </StackLayout>

            <StackLayout Style="{StaticResource stkBack}">
                <Button x:Name="btnBack" Clicked="btnBack_Clicked" Text="← Home" />
            </StackLayout>

        </StackLayout>
    </ContentPage.Content>
</ContentPage>
<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="IAD_Project.Views.ModuleOverviewPage">
    <ContentPage.Content>
        <StackLayout VerticalOptions="CenterAndExpand" HorizontalOptions="CenterAndExpand">

            <Label x:Name="lblModuleOverviewTitle" Text="" HorizontalTextAlignment="Center"/>
            <Label x:Name="lblModuleGrade" Text="" HorizontalTextAlignment="Center" />
            <Button x:Name="btnAddAssessment" Clicked="btnAddAssessment_Clicked" Text="Assessments: +" />
            
            <StackLayout x:Name="layout">
                
            </StackLayout>

            <Button x:Name="btnBACK" Clicked="btnBACK_Clicked" Text="← Year Overview" />

        </StackLayout>
    </ContentPage.Content>
</ContentPage>